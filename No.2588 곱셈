#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(){
	int numA, numB, numB1, numB10, numB100;
	scanf("%d", &numA);
	scanf("%d", &numB);

	numB100 = numB / 100;         // 3
	numB10 = (numB % 100) / 10;   // 8
 	numB1 = ((numB % 100) % 10);  // 5

	printf("%d\n", numA * numB1);
	printf("%d\n", numA * numB10);
	printf("%d\n", numA * numB100);
	printf("%d\n", numA * numB);

	return 0;
}
